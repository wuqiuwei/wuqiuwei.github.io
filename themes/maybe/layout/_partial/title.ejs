<% if (url == null || url == ""){ %>
    <% if (color == null || color == ""){ %>
      <div id="big-title" style="color: #999">
    <% }else { %>
      <div id="big-title" style="color:<%= color %>">
    <% } %>
<% }else { %>
  <% if (color == null || color == ""){ %>
    <div id="big-title" style="background-image:url(/img/<%= url %>);color: #ccc">
  <% }else { %>
    <div id="big-title" style="background-image:url(/img/<%= url %>); color:<%= color %>">
  <% } %>
<% } %>
